<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "../mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.cmfv.dao.ManagerMapper">

    <!--<resultMap id="select" type="com.baizhi.cmfv.bean.Manager">-->
