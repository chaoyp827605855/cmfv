        <!--<result column="m_name" property="mName"></result>-->
        <!--<result column="m_salt" property="mSalt"></result>-->
        <!--<result column="m_status" property="mStatus"></result>-->
        <!--<result column="m_password" property="mPassword"></result>-->
    <!--</resultMap>-->

    <select id="selectByName" resultType="Manager">
        select * from t_manager where m_name = #{mName}
    </select>

    <select id="selectManager" resultType="Manager">
        select * from t_manager where m_name = #{mName} and m_password = #{mPassword}
    </select>

    <insert id="insertManager">
        insert into t_manager values (#{mId},#{mName},#{mSalt},#{mStatus},#{mPassword})
    </insert>

</mapper>
